<h1>Matched Diagnoses</h1>

<% if @diagnoses.nil? %>
  <p>No diagnoses matched the selected symptoms.</p>
<% elsif @diagnoses.empty? %>
  <p>No diagnoses matched the selected symptoms.</p>
<% else %>
  <ul>
    <% @diagnoses.each do |diagnosis| %>
      <li><%= diagnosis.name %></li>
    <% end %>
  </ul>
<% end %>

<h1>Matched Specialties</h1>
<% if @specialty_names.present? %>
  <ul>
    <% @specialty_names.each do |name| %>
      <li><%= name %></li>
    <% end %>
  </ul>
<% else %>
  <p>No specialties matched the selected symptoms.</p>
<% end %>

